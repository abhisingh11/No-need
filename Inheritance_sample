class Student:
    def __init__(self,fname,lname,year):
        self.year = year
        self.fname = fname
        self.lname = lname
    def myqwe(self):
        print("Welcome Mr " + self.fname +self.lname+" of year "+ self.year) 
        print("You will graduate at " + str(int(self.year) +2))

name1 = input('Whats your first name?\n')
name2 = input('Whats your last name\n')
ye = input("When do you join the college\n")
x = Student(name1, name2, ye)
x.myqwe()

